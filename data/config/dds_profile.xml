<?xml version="1.0" encoding="UTF-8"?>
<dds xmlns="http://www.eprosima.com/XMLSchemas/fastRTPS_Profiles">
<profiles>
  <participant profile_name="dds-fmu-default" >
    <domainId>0</domainId>
    <rtps>
      <name>dds-fmu</name>
    </rtps>
    <!--builtin>
      <metatrafficUnicastLocatorList/> should contain one locator with null address and null port
      <initialPeersList>
        <locator>
          <udpv4>
            <address>10.2.0.155</address>
          </udpv4>
        </locator>
      </initialPeersList>
    </builtin-->
  </participant>

  <publisher profile_name="dds-fmu-default">
    <qos>
      <durability>
        <kind>TRANSIENT_LOCAL</kind>
      </durability>
      <reliability>
        <kind>RELIABLE</kind>
      </reliability>
    </qos>
  </publisher>

  <subscriber profile_name="dds-fmu-default">
    <qos>
      <durability>
        <kind>TRANSIENT_LOCAL</kind>
      </durability>
      <reliability>
        <kind>RELIABLE</kind>
      </reliability>
    </qos>
  </subscriber>

  <topic profile_name="Roundtrip" />

  <data_writer profile_name="Roundtrip">
    <qos>
      <reliability>
        <kind>RELIABLE</kind>
      </reliability>
    </qos>
  </data_writer>

  <data_reader profile_name="Roundtrip">
    <qos>
      <reliability>
        <kind>RELIABLE</kind>
      </reliability>
    </qos>
  </data_reader>
</profiles>

<log>
  <!-- dds-fmu implements a custom LogConsumer to output fast-dds logs to FMI -->
</log>

</dds>
