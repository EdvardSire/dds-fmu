<?xml version="1.0" encoding="utf-8" ?>
<fmiModelDescription
    fmiVersion="2.0"
    modelName="no.sintef.dds-fmu"
    description="DDS-FMI inter-communication"
    author="Joakim Haugen and Stian Skjong"
    copyright="Copyright 2023-2023, SINTEF Ocean"
    license="3-Clause BSD license"
    version="@CMAKE_PROJECT_VERSION@"
    guid="@FMU_UUID@"
    variableNamingConvention="structured"
    >

    <CoSimulation
        modelIdentifier="dds-fmu"
        canHandleVariableCommunicationStepSize="true"
        />

    <ModelVariables>
        <ScalarVariable name="x" valueReference="0" variability="continuous" causality="input">
          <Real start="1.0" />
        </ScalarVariable>
        <ScalarVariable name="y" valueReference="1" variability="continuous" causality="output"  initial="exact">
          <Real start="0.0" />
        </ScalarVariable>
    </ModelVariables>

    <ModelStructure>
        <Outputs>
            <Unknown index="2" />
        </Outputs>
    </ModelStructure>
</fmiModelDescription>
