    modelName="no.sintef.dds-fmu"
    description="DDS-FMI inter-communication"
    author="Joakim Haugen and Stian Skjong"
    copyright="Â© 2023 SINTEF Ocean"
    license="BSD-3-Clause"
    version="@CMAKE_PROJECT_VERSION@"
    guid="@FMU_UUID@"
    variableNamingConvention="structured"
    >

    <CoSimulation
        modelIdentifier="dds-fmu"
        canHandleVariableCommunicationStepSize="true"
        />

    <ModelVariables>
        <ScalarVariable name="y" valueReference="0" variability="discrete" causality="output" initial="exact">
          <Real start="0.0" />
        </ScalarVariable>
        <ScalarVariable name="x" valueReference="1" variability="discrete" causality="input">
          <Real start="1.0" />
        </ScalarVariable>
    </ModelVariables>

    <ModelStructure>
        <Outputs>
            <Unknown index="1" />
        </Outputs>
    </ModelStructure>
</fmiModelDescription>
